@model KPI_Schedule.Models.SettingUniversity

<h4>Ви впевнені, що хочете видалити?</h4>

<table>
    <tr>
        <th>Name</th>
        <th>ShortName</th>
        <th>Address</th>
        <th>Phone</th>
        <th>Site</th>
    </tr>
    <tr>
        <td>@Model.Name</td>
        <td>@Model.ShortName</td>
        <td>@Model.Address</td>
        <td>@Model.Phone</td>
        <td>@Model.Site</td>
    </tr>
</table>
<form asp-action="Delete" asp-route-id="@Model.Id">
    <input type="submit" value="Delete" />
</form>
<a asp-action="List">Go Back</a>